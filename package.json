{
  "name": "flap-admin-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "cd dist && node ./bin/www",
    "debug": "cd dist && node --nolazy --inspect-brk=6002 ./bin/www",
    "watch": "nodemon start",
    "build": "./build.sh",
    "build-local": "./build-local.sh",
    "build-mac": "sh build.sh",
    "build-local-mac": "sh build-local.sh",
    "type-check": "tsc --noEmit",
    "dev-mac": "nodemon -e js,ts --watch app --exec \"npm run build-local-mac && npm run start\"",
    "dev": "nodemon -e js,ts --watch app --exec \"npm run build-local && npm run start\"",
    "debug-dev-mac": "nodemon -e js,ts --watch app --exec \"npm run build-local-mac && npm run debug\"",
    "debug-dev": "nodemon -e js,ts --watch app --exec \"npm run build-local && npm run debug\"",
    "deploy-dev": "./deploy-dev.sh"
  },
  "dependencies": {
    "async": "^1.5.0",
    "aws-sdk": "^2.2.16",
    "awssum": "^1.2.0",
    "awssum-amazon": "^1.4.0",
    "awssum-amazon-s3": "^1.5.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.13.3",
    "cookie-parser": "^1.3.5",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.13.4",
    "file-type": "^11.0.0",
    "hashids": "0.3.3",
    "helmet": "^3.12.0",
    "jsonwebtoken": "^5.4.1",
    "knox": "^0.9.2",
    "lodash": "^4.17.11",
    "method-override": "^2.3.2",
    "moment": "^2.10.6",
    "mongoose": "^5.4.19",
    "morgan": "^1.9.1",
    "multer": "^1.3.0",
    "nconf": "^0.7.1",
    "nodemailer": "^1.8.0",
    "nodemailer-mandrill-transport": "^0.3.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "randomstring": "^1.1.3",
    "raven": "^2.6.3",
    "request": "^2.88.0",
    "sharp": "^0.22.1",
    "url-parse": "^1.4.4",
    "winston": "^2.4.2",
    "winston-daily-rotate-file": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.53",
    "@babel/core": "^7.0.0-beta.53",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.53",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.53",
    "@babel/preset-env": "^7.0.0-beta.53",
    "@babel/preset-typescript": "^7.0.0-beta.53",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "gulp": "^4.0.0",
    "nodemon": "^1.18.2",
    "prettier": "^1.10.2",
    "prettierrc": "^0.0.0-5",
    "typescript": "^2.9.1"
  }
}
